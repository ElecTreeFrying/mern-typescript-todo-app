{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "client": "cd .. & npm run start",
    "server": "tsnd src/app.ts",
    "server-all": "concurrently -c 'green,red,magenta' \"tsnd src/app.ts\" \"tsnd src/models/todo.model.ts\" \"tsnd src/routes/todo.ts\" ",
    "dev": "concurrently \"npm run server\" \"npm run prebuild\" \"npm run build\" \"npm run client\" \"cd server\" ",
    "dev2": "concurrently \"npm run server-all\" \"npm run prebuild\" \"npm run build\" \"npm run client\" \"cd server\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.7.0",
    "concurrently": "^5.1.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "description": ""
}
