{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "watch": false,
    "sourceMap": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "baseUrl": "src",
    "moduleResolution": "node",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/models",
    "src/routes",
    "src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
